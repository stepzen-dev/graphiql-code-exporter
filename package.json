{
  "name": "graphiql-code-exporter",
  "version": "4.0.0",
  "description": "Export working code snippets from GraphiQL queries",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "src",
    "es",
    "CodeExporter.css"
  ],
  "scripts": {
    "build:es": "babel -d es src --ignore __tests__ --extensions \".ts\"",
    "build:lib": "cross-env BABEL_ENV=commonjs babel -d lib src --ignore __tests__ --extensions \".ts\"",
    "build": "npm run build:es && npm run build:lib",
    "build:clean": "rm -r ./lib && mkdir ./lib && rm -r ./es && mkdir ./es",
    "prettier": "prettier --write \"src/**/*.ts\" \"docs/**/*.md\" README.md",
    "release": "npm run build && npm publish",
    "test": "jest"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "printWidth": 80
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OneGraph/graphiql-code-exporter.git"
  },
  "keywords": [
    "onegraph",
    "graphql",
    "graphiql",
    "tools",
    "devTools",
    "DX"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OneGraph/graphiql-code-exporter/issues"
  },
  "homepage": "https://github.com/OneGraph/graphiql-code-exporter#readme",
  "peerDependencies": {
    "codemirror": "^6",
    "graphql": "^16",
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "dependencies": {
    "@uiw/react-codemirror": "^4.15.1",
    "copy-to-clipboard": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "babel-jest": "^29.3.1",
    "codemirror": "^6.0.1",
    "cross-env": "^5.2.1",
    "graphql": "^16.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^1.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
